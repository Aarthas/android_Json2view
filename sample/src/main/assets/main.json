{
  "head": {
    "process": "AndroidLayout",
    "component": [
      {
        "name": "pane",
        "template": {
          "view": "TextView",
          "width": "250dp",
          "height": "60dp",
          "text": "&text",
          "gravity": "center",
          "margin": "10",
          "textSize": "18",
          "textColor": "#fff",
          "background": "&background",
          "visibility": "&visibility",
          "onClick": "jumpToActivity"
        }
      }
    ]
  },
  "template": {
    "view": "scrollView",
    "width": "match",
    "height": "match",
    "background": "#f6f6f6",
    "child": [
      {
        "view": "refreshBar"
      },
      {
        "view": "line"
      },
      {
        "name": "ver",
        "view": "verticalLayout",
        "width": "match",
        "height": "match",
        "gravity": "center",
        "child": [
          {
            "name": "name1",
            "view": "pane",
            "data": {
              "text": "从本地加载json文件渲染视图",
              "background": "#ff03A9F4",
              "route": "yiewdemo.JsonFromLocalActivity"
            }
          },
          {
            "name": "name2",
            "view": "pane",
            "data": {
              "background": "#ffF44336",
              "text": "从网络加载json文件渲染视图",
              "route": "yiewdemo.JsonFromNetActivity"
            }
          },
          {
            "view": "pane",
            "data": {
              "background": "#ff4CAF50",
              "text": "使用Yiew代码创建视图",
              "route": "yiewdemo.YiewCodeActivity"
            }
          },
          {
            "view": "pane",
            "data": {
              "background": "#fb9236",
              "text": "自定义component 用法 \n与 onClick事件",
              "route": "yiewdemo.FeatureActivity"
            }
          },
          {
            "view": "pane",
            "data": {
              "background": "#d9Ff5858",
              "text": "yiew虚拟节点的使用，更新虚拟节点，就能更新相应的view",
              "route": "yiewdemo.VirtualVNodeActivity"
            }
          },
          {
            "view": "pane",
            "data": {
              "background": "#d9Ff5858",
              "text": "head组件 的使用",
              "route": "yiewdemo.HeadComponentActivity"
            }
          },
          {
            "view": "pane",
            "data": {
              "background": "#d9Ff5858",
              "text": "image 的使用",
              "route": "yiewdemo.JsonImageActivity"
            }
          },
          {
            "view": "pane",
            "data": {
              "background": "#d9Ff5858",
              "text": "image 的使用",
              "route": "yiewdemo.SalesActivity"
            }
          }
        ]
      }
    ]
  }
}