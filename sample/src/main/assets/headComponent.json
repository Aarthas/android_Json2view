{
  "head": {
    "process": "AndroidLayout",
    "component": [
      {
        "name": "headComponent",
        "template": {
          "view": "RelativeLayout",
          "width": "match",
          "height": "48",
          "background": "#6fa8dc",
          "child": [
            {
              "view": "TextView",
              "text": "这是无参数的head组件",
              "textColor": "#666666",
              "marginLeft": "24",
              "textSize": "14",
              "layout_centerVertical": true
            }
          ]
        }
      },
      {
        "name": "headComponent2",
        "template": {
          "view": "RelativeLayout",
          "width": "match",
          "height": "48",
          "background": "#6fa8dc",
          "child": [
            {
              "view": "TextView",
              "text": "&text",
              "textColor": "&textColor",
              "textSize": "&textSize",
              "marginLeft": "24",
              "background": "&tvbackground",
              "layout_centerVertical": true
            },
            {
              "view": "ImageView",
              "width": "20",
              "height": "20",
              "marginLeft": "44",
              "src": "&img",
              "visibility": "visible",
              "background": "&tvbackground",
              "layout_centerVertical": true,
              "layout_alignParentRight": true
            }
          ]
        }
      }
    ]
  },
  "template": {
    "view": "verticalLayout",
    "width": "match",
    "height": "match",
    "background": "#f6f6f6",
    "child": [
      {
        "view": "refreshBar"
      },
      {
        "view": "scrollView",
        "width": "match",
        "height": "match",
        "visibility": "visible",
        "child": [
          {
            "view": "localComponent2",
            "data": {
              "text": "动态组件"
            }
          },
          {
            "view": "headComponent"
          },
          {
            "view": "headComponent2",
            "background": "#f03838",
            "data": {
              "text": "这是有参数的head组件",
              "textColor": "#fff",
              "textSize": "18",
              "tvbackground": "#e1e1e1",
              "img": "http://onpxz5rdd.bkt.clouddn.com/ic_arrow_right_666.png",
              "isShowArrow": "visible"
            }
          }
        ]
      }
    ]
  }
}