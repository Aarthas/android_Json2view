{
  "head": {
    "process": "AndroidLayout"
  },
  "template": {
    "view": "verticalLayout",
    "width": "match",
    "height": "match",
    "background": "#f6f6f6",
    "child": [
      {
        "view": "refreshBar"
      },
      {
        "view": "scrollView",
        "width": "match",
        "height": "match",
        "child": [
          {
            "view": "verticalLayout",
            "width": "match",
            "height": "wrap",
            "background": "#f6f6f6",
            "gravity": "center",
            "child": [
              {
                "view": "TextView",
                "text": "页面的简单使用",
                "textColor": "#444444",
                "padding": "16",
                "textSize": "16"
              },
              {
                "view": "TextView",
                "text": "从assets/complaintDetail.json加载数据",
                "textColor": "#444444",
                "padding": "16",
                "gravity": "center",
                "textSize": "14",
                "click": "gotoDataSource"
              }
            ]
          },
          {
            "name": "head1",
            "view": "RelativeLayout",
            "width": "match",
            "height": "48",
            "background": "#ffffff",
            "paddingLeft": "16",
            "paddingRight": "16",
            "child": [
              {
                "view": "ImageView",
                "src": "http://onpxz5rdd.bkt.clouddn.com/ic_service_black.png",
