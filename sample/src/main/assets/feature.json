{
  "head": {
    "process": "AndroidLayout",
    "component": [
      {
        "name": "headComponent",
        "template": {
          "view": "RelativeLayout",
          "width": "match",
          "height": "48",
          "background": "#6fa8dc",
          "child": [
            {
              "view": "TextView",
              "text": "这是一个head的template组件，又叫动态组件",
              "textColor": "#ffffff",
              "marginLeft": "24",
              "textSize": "14",
              "layout_centerVertical": true
            }
          ]
        }
      },
      {
        "name": "headComponent2",
        "template": {
          "view": "RelativeLayout",
          "width": "match",
          "height": "48",
          "background": "#6fa8dc",
          "child": [
            {
              "view": "TextView",
              "text": "&text",
              "textColor": "#ffffff",
              "marginLeft": "24",
              "textSize": "14",
              "layout_centerVertical": true
            }
          ]
        }
      }
    ]
  },
  "template": {
    "view": "verticalLayout",
    "width": "match",
    "height": "match",
    "background": "#f6f6f6",
    "child": [
      {
        "view": "refreshBar"
      },
      {
        "view": "scrollView",
        "width": "match",
        "height": "match",
        "child": [
          {
            "view": "localComponent2",
            "data": {
              "text": "本地组件"
            }
          },
          {
            "view": "localComponent"
          },
          {
            "view": "localComponent",
            "background": "#fb9236",
            "data": {
              "text": "background覆盖父控件，text覆盖子控件",
              "textColor": "#fff"
            }
          },
          {
            "view": "localComponent2",
            "data": {
              "text": "动态组件"
            }
          },
          {
            "view": "headComponent",

            "data": {
              "text": "动态组件"
            }
          },
          {
            "view": "headComponent2",
            "background": "#666666",
            "data": {
              "text": "这是动态组件，文字覆盖"
            }
          },
          {
            "view": "localComponent2",
            "data": {
              "text": "事件"
            }
          },
          {
            "view": "localComponent",
            "background": "#56b0f5",
            "data": {
              "text": "component事件,点击调用activity的showToast方法",
              "textColor": "#fff"
            },
            "click": "showToast"
          },
          {
            "click": "showToast",
            "view": "RelativeLayout",
            "width": "match",
            "height": "48",
            "background": "#ff0000",
            "paddingLeft": "16",
            "paddingRight": "16",
            "child": [
              {
                "view": "TextView",
                "text": "点击白色,产生子控件事件，\n点击红色,产生父控件事件",
                "height": "48",
                "width": "200",
                "textColor": "#444444",
                "marginLeft": "24",
                "textSize": "14",
                "gravity": "center",
                "click": "showToast2",
                "background": "#ffffff",
                "layout_centerVertical": true
              },
              {
                "view": "TextView",
                "width": "wrap",
                "text": ">",
                "textColor": "#444444",
                "layout_alignParentRight": "true",
                "layout_centerVertical": true
              }
            ]
          },
          {
            "view": "localComponent",
            "data": {
              "text": "application里，全局定义了一些component组件，查看代码便知",
              "textColor": "#666666"
            }
          },
          {
            "view": "localComponent2",
            "data": {
              "text": "图片适配"
            }
          },
          {
            "name": "imagePane",
            "view": "horizonLayout",
            "width": "match_parent",
            "height": "wrap_content",
            "background": "#ffffff",
            "paddingLeft": "12dp",
            "paddingRight": "12dp",
            "paddingTop": "16",
            "paddingBottom": "10dp",
            "child": [
              {
                "view": "ImageView",
                "src": "http://img.sanjiang.com/image/20173/1/image1488349927926.jpg",
                "width": "74dp",
                "height": "74dp",
                "margin": "5dp"
              },
              {
                "view": "ImageView",
                "src": "https://ss0.baidu.com/6ONWsjip0QIZ8tyhnq/it/u=232163874,2153671052&fm=58",
                "width": "74dp",
                "height": "74dp",
                "margin": "5dp"
              },
              {
                "view": "ImageView",
                "src": "https://ss0.baidu.com/6ONWsjip0QIZ8tyhnq/it/u=232163874,2153671052&fm=58",
                "width": "74dp",
                "height": "74dp",
                "margin": "5dp"
              },
              {
                "view": "ImageView",
                "src": "https://ss0.baidu.com/6ONWsjip0QIZ8tyhnq/it/u=232163874,2153671052&fm=58",
                "width": "74dp",
                "height": "74dp",
                "margin": "5dp"
              }
            ]
          }
        ]
      }
    ]
  }
}