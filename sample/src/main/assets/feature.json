{
  "head": {
    "process": "AndroidLayout",
    "component": [
      {
        "name": "pane",
        "template": {
          "name": "headComponent",
          "template": {
            "view": "RelativeLayout",
            "width": "match",
            "height": "48",
            "background": "#6fa8dc",
            "child": [
              {
                "view": "TextView",
                "text": "这是一个head的template组件，又叫动态组件",
                "textColor": "#666666",
                "marginLeft": "24",
                "textSize": "14",
                "layout_centerVertical": true
              }
            ]
          }
        }
      },
      {
        "name": "headComponent2",
        "template": {
          "view": "RelativeLayout",
          "width": "match",
          "height": "48",
          "background": "#6fa8dc",
          "child": [
            {
              "view": "TextView",
              "text": "&text",
              "textColor": "&textColor",
              "textSize": "&textSize",
              "marginLeft": "24",
              "background": "&tvbackground",
              "layout_centerVertical": true
            },
            {
              "view": "ImageView",
              "width": "20",
              "height": "20",
              "marginLeft": "44",
              "src": "&img",
              "visibility": "&isShowArrow",
              "background": "&tvbackground",
              "layout_centerVertical": true,
              "layout_alignParentRight": true
            }
          ]
        }
      }
    ]
  },
  "template": {
    "view": "verticalLayout",
    "width": "match",
    "height": "match",
    "background": "#f6f6f6",
    "child": [
      {
        "view": "refreshBar"
      },
      {
        "view": "scrollView",
        "width": "match",
        "height": "match",
        "child": [
          {
            "view": "localComponent2",
            "data": {
              "text": "本地组件"
            }
          },
          {
            "view": "localComponent"
          },
          {
            "view": "localComponent",
            "background": "#fb9236",
            "data": {
              "text": "background覆盖父控件，text覆盖子控件",
              "textColor": "#fff"
            }
          },
          {
            "view": "localComponent2",
            "data": {
              "text": "动态组件"
            }
          },
          {
            "view": "headComponent",
            "data": {
              "text": "动态组件a"
            }
          },
          {
            "name": "name1",
            "view": "headComponent2",
            "background": "#f03838",
            "data": {
              "text": "这是动态组件，属性覆盖",
              "textColor": "#fff",
              "textSize": "18",
              "tvbackground": "#e1e1e1",
              "img": "http://onpxz5rdd.bkt.clouddn.com/ic_arrow_right_666.png",
              "isShowArrow": "visible"
            }
          },
          {
            "name": "name2",
            "view": "headComponent2",
            "visibility": "visible",
            "data": {
              "text": "属性覆盖",
              "textColor": "#fff",
              "textSize": "15",
              "tvbackground": "#e1e1e1",
              "img": "http://onpxz5rdd.bkt.clouddn.com/ic_arrow_right_666.png",
              "isShowArrow": "gone"
            }
          },

          {
            "view": "localComponent2",
            "data": {
              "text": "事件"
            }
          },
          {
            "view": "localComponent",
            "background": "#56b0f5",
            "data": {
              "text": "component事件,点击调用activity的showToast方法"
            },
            "click": "showToast"
          },
          {
            "click": "showToast",
            "view": "RelativeLayout",
            "width": "match",
            "height": "48",
            "background": "#ff0000",
            "paddingLeft": "16",
            "paddingRight": "16",
            "child": [
              {
                "view": "TextView",
                "text": "点击白色,产生子控件事件，\n点击红色,产生父控件事件",
                "height": "48",
                "width": "200",
                "textColor": "#444444",
                "marginLeft": "24",
                "textSize": "14",
                "gravity": "center",
                "click": "showToast2",
                "background": "#ffffff",
                "layout_centerVertical": true
              },
              {
                "view": "TextView",
                "width": "wrap",
                "text": ">",
                "textColor": "#444444",
                "layout_alignParentRight": "true",
                "layout_centerVertical": true
              }
            ]
          },
          {
            "view": "localComponent",
            "data": {
              "text": "application里，全局定义了一些component组件，查看代码便知",
              "textColor": "#666666"
            }
          }
        ]
      }
    ]
  }
}